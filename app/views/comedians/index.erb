<% @comedians.each do |comedian| %>
  <section class="comedians" id="comedian-<%= comedian.id %>">
    <article class="comedian-info">
      <img src="<%= comedian.headshot %>" alt="Photo Of <%= comedian.name %>">
      <h6><%= comedian.name %></h6>
      <p>Age: <%= comedian.age %></p>
      <p>City: <%= comedian.birthplace %></p>
    </article>
  </section>
<% end %>
